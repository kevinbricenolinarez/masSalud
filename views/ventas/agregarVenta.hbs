<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Registrar Venta</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Venta
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <form role="form">

                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>RUT Cliente</label>
                                            <input class="form-control" id="venCliRut" maxlength="11" required
                                                placeholder="26263200-8">
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>RUT Vendedor</label>
                                            <input class="form-control" id="venVenRut" maxlength="11" required
                                                placeholder="26263200-8">
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Nombre MED</label>
                                            <select class="form-control" id="venMed" maxlength="20" required>
                                                {{#each medicamentos}}
                                                <option value="{{IdMed}}">{{NomMed}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Formato MED</label>
                                            <select class="form-control" id="venFor" maxlength="20" required>
                                                {{#each formatos}}
                                                <option value="{{TipoFormato}}">{{Descripcion}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Presentaci√≥n MED</label>
                                            <select class="form-control" id="venTam" maxlength="20" required>
                                                {{#each presentaciones}}
                                                <option value="{{TipoPresentacion}}">{{TipoPresentacion}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-3">
                                        <div class="form-group">
                                            <label>Tipo MED</label>
                                            <select class="form-control" id="venTam" maxlength="20" required>
                                                {{#each tipos}}
                                                <option value="{{TipoMedic}}">{{TipoMedic}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <button class="btn btn-success">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /.container-fluid -->